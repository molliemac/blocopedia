<h1>Wikis</h1>
<% @wikis.each do |wiki| %>
<div class="clearfix">
  <div class="col-md-6 index-btn">
    <h3>
      <%= link_to wiki.title, { :controller => "wikis", :action => "show", :id => wiki.id }, class: "col-md-6" %>
    </h3>
  </div>
  <div class="col-md-1 index-btn">
    <%= link_to "edit", edit_wiki_path(wiki) %>
  </div>
  <div class="col-md-1 index-btn">
    <%= link_to "delete", wiki_path(wiki), method: :delete, data: { confirm: "Are you sure you want to delete this wiki?" } %>
  </div>
</div>
<% end %>
<div class="new-wiki">
  <%=link_to "New Wiki", new_wiki_path, class: "btn btn-default" %>
</div>